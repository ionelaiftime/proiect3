<header  >
    <% if(utilizator!=null) {%>
    <h1 class="log">
        Logat ca <%- utilizator.prenume-%> <%-utilizator.nume-%>!
        <form action="logout" method="get">
            <input type="submit" value="logout" class="btnlogout" /> 
        </form>
    </h1>
    <%} %>
   
</header>
<main>
        
    <h1 class="titlu">Chestionar</h1>

   
    <form  class="chestionar" action="/rezultat-chestionar" method="POST">
        <% for(let i=0;i<intrebari.length;i++){ %>
        <p><%- intrebari[i].intrebare %></p>
        <% for(let j=0;j<intrebari[i].variante.length;j++){ %>
            <input type="radio" name="q <%- i %>" value="<%- j %>"/><%- intrebari[i].variante[j] %> <br/>
            <%} %>
        <% } %>
            <input type="submit" value="trimite" class="btnsubmit"/>    
    </form>
       
    
</main>
        
